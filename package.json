  "author": {
    "name": "Matt Godbolt",
    "email": "matt@godbolt.org",
    "url": "http://xania.org"
  },
  "repository": {
    "type": "git",
    "url": "mattgodbolt/compiler-explorer"
  },
  "version": "0.0.3",
